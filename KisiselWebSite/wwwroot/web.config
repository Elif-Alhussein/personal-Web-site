<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		</handlers>
		<aspNetCore processPath="dotnet" arguments=".\KisiselWebSite.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
	</system.webServer>
	<connectionStrings>
		<add name="Context" 
			 connectionString="Data Source=(localdb)\MSSQLLocalDB; 
			 Initial Catalog=KisiselWebSiteDb; 
			 Integrated Security=true;" 
			 providerName="System.Data.SalClient" />
	</connectionStrings>
</configuration>
